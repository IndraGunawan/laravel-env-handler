{
    "name": "indragunawan/laravel-env-handler",
    "description": "Laravel 5 artisan command to handling the environment configuration (.env)",
    "keywords": ["laravel", "environment", "configuration", "artisan", "dotenv"],
    "license": "MIT",
    "authors": [
        {
            "name": "Indra Gunawan",
            "email": "hello@indra.my.id"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/console": "5.0.*",
        "vlucas/phpdotenv": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "IndraGunawan\\": "src/IndraGunawan/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
